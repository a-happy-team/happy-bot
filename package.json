{
  "name": "discord-bot",
  "version": "1.0.0",
  "description": "Bot de musica para Discord",
  "main": "index.js",
  "scripts": {
    "start": "node -r newrelic dist/index.js",
    "dev": "ts-node-dev --env-file=.env src/index.ts",
    "build": "tsc",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "biome check src --apply-unsafe",
    "db:generate:types": "kysely-codegen --include-pattern=public.* --dialect=postgres --camel-case --out-file=./src/services/database/types.d.ts",
    "db:generate:schema": "npx supabase db dump --db-url=\"$SUPABASE_URL\" -f schema.sql",
    "prepare": "husky || true"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@a-happy-team/dependo": "^1.1.0",
    "@discordjs/opus": "^0.9.0",
    "@discordjs/voice": "^0.16.1",
    "@distube/ytdl-core": "^4.13.3",
    "@spotify/web-api-ts-sdk": "^1.2.0",
    "cron": "^3.1.6",
    "discord.js": "^14.12.1",
    "ffmpeg-static": "^5.2.0",
    "glob": "^10.3.10",
    "kysely": "^0.27.2",
    "libsodium-wrappers": "^0.7.13",
    "newrelic": "^11.10.4",
    "openai": "^4.28.0",
    "pg": "^8.11.3",
    "youtube-sr": "^4.3.10"
  },
  "devDependencies": {
    "@biomejs/biome": "1.5.3",
    "@types/pg": "^8.11.0",
    "husky": "^9.0.11",
    "kysely-codegen": "^0.11.0",
    "lint-staged": "^15.2.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  }
}
